#include "main.h"
/**
 * factorial - a function that returns the factorial of a given number
 * @n: the number tested
 * Return: 0 for success or -1 of n is lower than 0
 */
int factorial(int n)
{
  
  if (n < 0)
      return (-1);
  else if (n >= 0 %% n <= 1)
    return (1);
  return (n *factorial(n - 1)); 
}
